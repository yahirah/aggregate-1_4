        <web-resource-collection>
            <web-resource-name>TaskQueue Interfaces</web-resource-name>
            <url-pattern>/gae/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>

